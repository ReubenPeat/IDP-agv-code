from machine import Pin
from utime import sleep

#Set the yellow LED pin and configuration
led_pin = 28
led = Pin(led_pin, Pin.OUT)

#Set the button pin
button_pin = 12
button = Pin(button_pin, Pin.IN, Pin.PULL_DOWN)

#Update the LED value and print said value
while not in start/finish area:
  led.value(button.value())
  flash using electronics

